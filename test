import random
randomint = random.randint(1,20)


for n in range(5):
    print('Take a guess')
    guessint = int(input())
    if guessint > randomint:
            print('Too high')
    elif int(guessint) < randomint:
            print('Too low')
    else:
        print('You got it! ')
        break
    
if guessint == randomint:
    print('You took ' + str(n) + ' guesses!')
else:
    print('Try again next time')
